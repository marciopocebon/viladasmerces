{
  "name": "viladasmerces",
  "description": "business directory listing",
  "version": "0.0.1",
  "homepage": "https://github.com/marioluan/viladasmerces",
  "email": "mariosouzaluan@gmail.com",
  "author": "Mario Luan <mariosouzaluan@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/marioluan/viladasmerces"
  },
  "dependencies": {
    "express": ">=3.x",
    "mongodb": ">=1.3"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "scripts": {
    "prestart"  : "sudo mongod --quiet --dbpath ~/www/viladasmerces/data/db &",
    "start"     : "node ~/www/viladasmerces/backend/index.js &",
    "prestop"   : "sudo killall -r -v mongo &",
    "stop"      : "sudo killall -r -v node &"
  },
  "private": true,
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-contrib-uglify": "~0.2.4",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-contrib-compass": "~0.5.0",
    "grunt-shell": "~0.3.1"
  }
}
